	} else {
		validate_format(!!(object = object_define(OTP_DOUBLE, sizeof(double_t))), 
			"unable to alloc memory!");
		*(double_t *)object->ptr = *(double_t *)esp->ptr * *(double_t *)tr->object->ptr;
	}

	tr->object = object;

	return c->next;
}

iarray_t *
decode_div(decode_t *tr, iarray_t *c) {
